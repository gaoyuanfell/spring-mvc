<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="moka.user.dao.UserDao">
    <insert id="insert" parameterType="moka.user.bo.UserEntity">
      INSERT INTO USER(user,password,email,address,phone) VALUES (#{user}, #{password}, #{email}, #{address}, #{phone})
    </insert>
</mapper>